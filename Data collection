import csv

listTrainingData = []
listPredictionModel = []
listDetectionModel = []

with open('100_data.csv', 'r') as csvFile:
  reader = csv.reader(csvFile)
  for row in reader:
    listTrainingData.append(row)

csvFile.close()

listPredictionModel = listTrainingData
listDetectionModel = listTrainingData

